{
    "name": "pseudocode-language-support",
    "displayName": "Pseudocode Language Support",
    "description": "",
    "version": "1.0.3",
    "engines": {
        "vscode": "^1.53.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "pseudo",
            "aliases": ["Pseudocode", "pseudo"],
            "extensions": [".pseudo"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "pseudo",
            "scopeName": "source.pseudo",
            "path": "./syntaxes/pseudo.tmLanguage.json"
        }]
    }
}